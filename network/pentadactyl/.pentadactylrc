map -count -modes=n,v N -builtin <count>H
map -count -modes=n,v K -builtin <count>N
map -count -modes=n,v e -builtin <count>j
map -count -modes=n,v u -builtin <count>k
map -count -modes=n,v l -builtin <count>u
map -count -modes=n,v i -builtin <count>l
map -count -modes=n,v j -builtin <count>i
map -count -modes=n,v J -builtin <count>I
map -count -modes=n,v I -builtin <count>L
map -count -modes=n,v X -builtin <count>:emenu<Space>Private<Space>Browsing<Tab>
